# =============================================================================
# WebCoder25 - Environment Variables
# Copy this file to .env and fill in your values
# =============================================================================

# API Configuration (Digital Ocean Backend)
VITE_API_URL=http://localhost:8000

# Neon Auth Configuration
VITE_NEON_AUTH_URL=https://your-project.neonauth.region.aws.neon.tech/neondb/auth

# Neon Database (for reference - backend uses this)
# DATABASE_URL=postgresql://user:pass@host.neon.tech/db?ssl=require

# App Configuration
VITE_APP_NAME=WebCoder25
VITE_APP_VERSION=2.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_AI_ASSISTANT=true

# Development Settings
VITE_PORT=3000
